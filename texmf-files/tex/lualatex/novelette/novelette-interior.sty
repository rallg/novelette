%% Tacked onto the end of AtBeginDocument, just prior to document body:
\AfterEndPreamble{ % Deactivate many special characters and macros.
  \gdef\input#1{%
    \unskip%
    \nvt@err{Cannot use \string\input, page \this@page}%
      {Novelette does not allow \string\input\space in document body.^^J%
       Only \string\subdoc, \string\image, and \string\icon, may be used.}%
  }
  \catcode`\$=12
  \catcode`\_=12
  \catcode`\#=12
  \catcode`\&=12
  \catcode`\^=12
  \def\({(}
  \def\){)}
  \def\[{[}
  \def\]{]}
  \ifthenelse{\equal{\@mode}{fill}}{\filler@text}{}
}

%% Finish the setup, with some values not available until here:
\AtBeginDocument{
  \global\deflength\textfloatsep{0pt plus \@leading minus \@leading}
  \global\deflength\parindent{\p@rindent}
  \gdef\forceindent{%
    \ifvmode\else\unskip\fi%
    \rule{0pt}{.02pt}\hspace{\p@rindent}%
  }
  \gdef\backindent{\ifvmode\else\unskip\fi\hspace{-\p@rindent}}
  \ifthenelse{\equal{\@mode}{final}}{\global\deflength\overfullrule{0pt}}{}
  \DeclareDocumentCommand\textls{sO{}+m}{#3} % Do-nothing.
  \let\lsstyle\relax
  \ifthenelse{\equal{\@mode}{shade}\OR\equal{\@mode}{fill}%
    \OR\equal{\@mode}{dev}}{\RequirePackage{novelette-shading}
  }{}
  \hyphenpenalty=1000
  \exhyphenpenalty=1000
  \ifdefined\preamble@file
    \gdef\verso@head{(subfile)}
    \gdef\recto@head{(subfile)}
  \else
    \gdef\verso@head{}
    \gdef\recto@head{}
  \fi
\set@protrusions %%%%%
  \pagestyle{normal} % Use \frontmatter at start.
  \gdef\include#1{\@lert{Ignored \string\include{#1}, page \this@page.}}
}
